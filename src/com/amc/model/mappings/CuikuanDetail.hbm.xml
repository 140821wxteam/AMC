<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>  
    <class name="com.amc.model.models.CuikuanDetail" table="t_cuikuandetail" lazy="false">  
	    
	   	<id name="id" column="id" type="integer" >
			<generator class="native" />
		</id>
		
		<version name="version" column="version" type="integer"/>
	    <property name="enable" column="enable" type="boolean" not-null="true"/>
		<property name="cuikuanId" column="cuikuanId" type="string" not-null="true"/>
	    <property name="productId" column="productId" type="string" not-null="true"/>
	    <property name="productName" column="productName" type="string" not-null="false"/>
	    <property name="factoryId" column="factoryId" type="string" not-null="false"/>  
	    <property name="num" column="num" type="integer" not-null="false"/>
	    <property name="price" column="price" type="double" not-null="false"/>
	    <property name="money" column="money" type="double" not-null="false"/>
		
    </class>  
</hibernate-mapping> 